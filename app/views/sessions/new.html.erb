<%= render "/shared/header" %>
<h3>Log in</h3>

<%= form_tag sessions_path do %>
	<div class="field">
		<%= label_tag :email %></br>
		<%= text_field_tag :email, params[:email] %></br></br>
	</div>
	<div class="field">
		<%= label_tag :password %></br>
		<%= password_field_tag :password %></br></br>
	</div>
	<div class="actions">
		<%= submit_tag "Log in" %>
	</div>
<% end %>